{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    },
    {
      "pattern": "apps/*/"
    },
    {
      "pattern": "packages/*/"
    },
    {
      "pattern": "templates/*/"
    }
  ],
  "tailwindCSS.experimental.classRegex": [
    [
      "cva\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]"
    ],
    [
      "cn\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]"
    ]
  ],
  "cSpell.words": [
    "Flowless"
  ]
}